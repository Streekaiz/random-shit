local Tab = Window:MakeTab({ -- Scripts 1
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddLabel("Im still working on this, so not many features.")
Tab:AddLabel("Also any games with an anticheat will detect this.")

Tab:AddSlider({ -- Walkspeed
	Name = "Walkspeed",
	Min = 2,
	Max = 150,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

Tab:AddSlider({ -- Jump Power
	Name = "Jump Power",
	Min = 2,
	Max = 300,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Power",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end    
})

Tab:AddButton({ -- Kick
	Name = "Infinite Jump",
	Callback = function()
      		print("button pressed")
loadstring(game:HttpGet("https://pastebin.com/raw/vbNMmEJC", true))()
  	end    
})

Tab:AddButton({ -- CTRL TP
    Name = "Ctrl TP",
    Callback = function()
              print("button pressed")
local Plr = game:GetService("Players").LocalPlayer
local Mouse = Plr:GetMouse()
 
Mouse.Button1Down:connect(
    function()
        if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then
            return
        end
        if not Mouse.Target then
            return
        end
        Plr.Character:MoveTo(Mouse.Hit.p)
    end
)
      end    
})

Tab:AddParagraph("Ctrl TP","Hold CTRL and click wherever to tp to your cursor.")

Tab:AddButton({ -- Kick
	Name = "Kick Yourself",
	Callback = function()
      		print("button pressed")
Game:GetService("Players").LocalPlayer:Kick("You have been kicked due to unexpected client behavior.")
  	end    
})

Tab:AddParagraph("Kick Yourself","Have no idea why you need this.")

Tab:AddButton({ -- FPS Cap 
	Name = "Boost FPS (sets fps cap to 360)",
	Callback = function()
      		print("button pressed")
setfpscap(360)
  	end    
})



